{
  "name": "@maticnetwork/maticjs",
  "version": "3.8.3-beta.1",
  "description": "Javascript developer library for interacting with Matic Network",
  "main": "dist/npm.export.js",
  "types": "dist/ts/index.d.ts",
  "browser": "dist/matic.umd.js",
  "react-native": "dist/matic.node.js",
  "files": [
    "lib",
    "dist",
    "artifacts",
    "types"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "webpack --env build",
    "build": "tsc",
    "build:link": "npm run build:webpack && npm link",
    "deploy": "npm run clean && npm run build:webpack",
    "prepublishOnly": "npm run deploy",
    "lint": "tslint src/**/*.ts",
    "lint:fix": "tslint src/**/*.ts --fix",
    "build:test": "npm run build:link && npm run test",
    "test": "cd test && npm run install:lib:test",
    "debug": "npm run build:pack && cd test && npm run link:lib:debug",
    "testrpc": "ganache-cli --mnemonic 'clock radar mass judge dismiss just intact mind resemble fringe diary casino' --gasLimit 13000000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maticnetwork/matic.js.git"
  },
  "keywords": [
    "ethereum",
    "web3",
    "ethers",
    "matic"
  ],
  "author": "Jaynti Kanani <jd@matic.network>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maticnetwork/matic.js/issues"
  },
  "homepage": "https://github.com/maticnetwork/matic.js#readme",
  "dependencies": {
    "@ethereumjs/block": "^3.6.2",
    "assert": "^2.1.0",
    "ethereumjs-util": "^7.1.4",
    "merkle-patricia-tree": "^4.2.4",
    "node-fetch": "^2.6.1",
    "stream": "^0.0.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "cross-env": "^7.0.3",
    "husky": "2.4.1",
    "lint-staged": "^15.2.7",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.6.3"
  },
  "browserslist": [
    "> 1%",
    "node 8",
    "not dead"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build"
    }
  }
}
